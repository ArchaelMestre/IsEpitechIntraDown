{
  "name": "Is Epitech Intra Down",
  "version": "1.0",
  "description": "Chrome extension to know if the EPITECH intranet is down or not.",
  "icons": {
    "100": "Icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "https://intra.epitech.eu/",
    "https://google.com/"
  ],
  "manifest_version": 2
}
